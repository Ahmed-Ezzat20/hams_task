# LiveKit Configuration
LIVEKIT_URL=wss://your-livekit-server.com
LIVEKIT_API_KEY=your_api_key
LIVEKIT_API_SECRET=your_api_secret

# ElevenLabs Configuration (for STT)
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Google Configuration (for Gemini LLM)
GOOGLE_API_KEY=your_google_api_key

# Arabic EOU Model Configuration
# Path to the quantized ONNX model for best performance
ARABIC_EOU_MODEL_PATH=./eou_model/models/eou_model_quantized.onnx

# Optional: HuggingFace Model (if you uploaded to HuggingFace)
# The agent will download the model automatically
# ARABIC_EOU_MODEL_HF_REPO=your-username/arabic-eou-detector
# ARABIC_EOU_MODEL_HF_FILENAME=model_quantized.onnx

# Turn Detector Configuration
# Threshold for EOU detection (0.0 to 1.0)
# Lower = more sensitive (detects EOU earlier)
# Higher = less sensitive (waits longer before detecting EOU)
ARABIC_EOU_THRESHOLD=0.7
